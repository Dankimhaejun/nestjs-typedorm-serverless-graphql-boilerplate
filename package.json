{
  "name": "nestjs-typedorm-serverless-graphql-boilerplate",
  "version": "0.0.1",
  "keywords": ["nestjs", "typedorm", "serverless", "graphql"],
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "delete:local:table":"aws dynamodb delete-table --table-name master --endpoint-url http://localhost:8000",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e .env.development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "deploy:staging": "npm run build && npx sls deploy --stage staging",
    "remove:staging": "npx sls remove --stage staging",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev:backup": "nest start --watch",
    "start:debug:backup": "nest start --debug --watch"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.209.0",
    "@aws-sdk/util-dynamodb": "^3.209.0",
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@typedorm/common": "^1.15.2",
    "@typedorm/core": "^1.15.2",
    "@vendia/serverless-express": "^4.10.1",
    "apollo-server-express": "^3.11.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.2.1",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.13.2",
    "clayful": "^2.5.0",
    "graphql": "^16.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/aws-lambda": "^8.10.108",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.14",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-offline": "^11.3.0",
    "serverless-webpack": "^5.11.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
